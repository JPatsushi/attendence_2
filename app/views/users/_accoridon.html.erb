<div class="panel-group" id="<%= "accordion-#{id}" %>" role="tablist" aria-multiselectable="true">
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="<%= "heading-#{id}" %>">
      <h4 class="panel-title">
        <button data-toggle="collapse" data-parent="#accordion" href="#<%= "collapse-#{id}" %>" aria-expanded="false" aria-controls="#<%= "collapse_#{id}" %>" class="btn btn-primary accordion">編集</button>
      </h4>
    </div>
    <div id="<%= "collapse-#{id}" %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="<%= "heading-#{id}" %>">
      <div class="panel-body">
        <%= form_for(user, url: user_path(user)) do |f| %>
        <!--<%#= render 'shared/error_messages', object: f.object %>-->
  
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control' %>
  
        <%= f.label :email %>
        <%= f.email_field :email, class: 'form-control' %>
        
        <%= f.label :depart %>
        <%= f.text_field :depart, class: 'form-control' %>
  
        <%= f.label :employee_number %>
        <%= f.number_field :employee_number, class: 'form-control' %>
        
        <%= f.label :uid %>
        <%= f.text_field :uid, class: 'form-control' %>
 
        <%= f.label :password %>
        <%= f.password_field :password, class: 'form-control' %>
  
        <%= f.label :basic_work_time %>
        <%= f.time_field :basic_work_time, class: 'form-control' %>
        
        <%= f.label :designated_work_start_time %>
        <%= f.time_field :designated_work_start_time, class: 'form-control' %>
        
        <%= f.label :designated_work_end_time %>
        <%= f.time_field :designated_work_end_time, class: 'form-control' %>
  
        <%= f.submit "編集", class: "btn btn-primary" %>
      <% end %>
        
    
        
      </div>
    </div>
  </div>
</div>  